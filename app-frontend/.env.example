# AarogyaRekha Frontend Environment Configuration
# Copy this file to .env and fill in your actual values
# DO NOT COMMIT .env TO VERSION CONTROL

# Backend API Configuration
API_BASE_URL=http://localhost:8000/api/

# Google Services Configuration
# Get these values from Google Cloud Console > APIs & Services > Credentials
GOOGLE_ANDROID_CLIENT_ID=your_android_client_id_here.apps.googleusercontent.com
GOOGLE_WEB_CLIENT_ID=your_web_client_id_here.apps.googleusercontent.com
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here

# App Configuration
APP_NAME=AarogyaRekha
APP_VERSION=1.0.0
APP_PACKAGE_NAME=com.aarogyarekha.app

# Development Settings
DEBUG_MODE=true
ENABLE_LOGGING=true

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_LOCATION_TRACKING=true

# External Services (Optional)
WEATHER_API_KEY=your_weather_api_key_here
SATELLITE_DATA_API_KEY=your_satellite_api_key_here

# Security Settings
ENCRYPT_LOCAL_DATA=true
SESSION_TIMEOUT=3600

# Localization
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,hi,bn,te,ta,mr